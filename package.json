{
	"name": "@vafast/jwt",
	"version": "0.0.1",
	"description": "Plugin for Vafast for JWT authentication",
	"license": "MIT",
	"scripts": {
		"dev": "bun run --watch example/index.ts",
		"test": "bun test",
		"build": "bun build.ts",
		"release": "npm run build && npm run test && npm publish --access public"
	},
	"peerDependencies": {
		"vafast": ">= 0.1.12"
	},
	"dependencies": {
		"jose": "^5.0.0",
		"@sinclair/typebox": "^0.34.40"
	},
	"devDependencies": {
		"@types/bun": "1.1.14",
		"eslint": "9.6.0",
		"tsup": "^8.1.0",
		"typescript": "^5.5.3"
	},
	"main": "./dist/cjs/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/cjs/index.js"
		}
	},
	"keywords": [
		"vafast",
		"jwt",
		"authentication"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/vafastjs/vafast-jwt"
	},
	"author": {
		"name": "saltyAom",
		"url": "https://github.com/SaltyAom",
		"email": "saltyaom@gmail.com"
	},
	"homepage": "https://github.com/vafastjs/vafast-jwt",
	"bugs": "https://github.com/vafastjs/vafast-jwt/issues"
}
